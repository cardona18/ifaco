<?xml version="1.0" encoding="UTF-8" ?>
<openerp>
    <data>

        <!-- HR PTU LINE VIEWS -->

        <record id="hr_ptu_line_action" model="ir.actions.act_window">
            <field name="name">HR PTU LINE</field>
            <field name="res_model">hr.ptu.line</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record model="ir.ui.view" id="hr_ptu_line_form">
            <field name="name">hr_ptu_line_form</field>
            <field name="model">hr.ptu.line</field>
            <field name="arch" type="xml">
                <form name="HR PTU LINE">
                    <group>
                        <field name="wizard_id"/>
                    </group>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="hr_ptu_line_tree">
            <field name="name">"hr_ptu_line_tree"</field>
            <field name="model">hr.ptu.line</field>
            <field name="arch" type="xml">
                <tree name="HR PTU LINE">
                    <field name='wizard_id'/>
                </tree>
            </field>
        </record>

        <!-- <menuitem id="hr_ptu_line_menu" name="HR PTU LINE" action="hr_ptu_line_action" sequence="10"/> -->

    </data>
</openerp>
